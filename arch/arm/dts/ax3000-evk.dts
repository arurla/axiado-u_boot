/dts-v1/;

#include "ax3000.dtsi"

/ {
	model = "Axiado AX3000-EVK";
	compatible = "axiado,AX3000";
	#address-cells = <2>;
#size-cells = <2>;

	chosen {
		bootargs = "console=ttyPS0,115200 earlyprintk earlycon nr_cpus=4 maxcpus=4";
        stdout-path = "serial0:115200";
	};

	memory@0 {
			device_type = "memory";
			reg = <0x00 0x3C000000 0x00 0x1000000>;
	};
};

&uart0{
	status = "Okay";
};
